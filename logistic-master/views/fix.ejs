<%include header.ejs%>
<div class="hd">
  <h1 class="page_title">故障报修</h1>
</div>
<div class="weui_cells weui_cells_form">
  <div class="weui_cell">
    <div class="weui_cell_hd"><label class="weui_label">宿舍号</label></div>
    <div class="weui_cell_bd weui_cell_primary">
      <input id="sushehao" class="weui_input" placeholder="请输入宿舍号" value='<%=domId%>'/>
    </div>
  </div>
  <div class="weui_cell">
    <div class="weui_cell_hd"><label class="weui_label">故障描述</label></div>
    <div class="weui_cell_bd weui_cell_primary">
      <input id="fixreason" class="weui_input" placeholder="请输入详细故障描述"/>
    </div>
  </div>
  <div class="weui_cell">
    <div class="weui_cell_hd"><label class="weui_label">备注</label></div>
    <div class="weui_cell_bd weui_cell_primary">
      <textarea id="beizhu" class="weui_textarea" placeholder="..." rows="3"></textarea>
    </div>
  </div>
</div>
<div class="weui_btn_area">
  <a class="weui_btn weui_btn_primary" id="commitfix">提交</a>
</div>
<div id="alertinfo" class="weui_dialog_alert" style="display: none;">
  <div class="weui_mask"></div>
  <div class="weui_dialog">
    <div id="1" class="weui_dialog_hd"><strong class="weui_dialog_title"></strong></div>
    <div id="2" class="weui_dialog_bd"></div>
    <div class="weui_dialog_ft">
      <a class="weui_btn_dialog primary" href='/main'>返回</a>
    </div>
  </div>
</div>
<%include footer.ejs%>
