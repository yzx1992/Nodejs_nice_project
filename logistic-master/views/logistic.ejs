<%include header.ejs%>
<div class="hd">
  <h1 class="page_title">故障报修&送水</h1>
</div>
<div>
  <% if(reqData===""){%>
    <div class="weui_media_box weui_media_text">
        <p class="weui_media_desc">今天还没有新的请求！</p>
    </div>
  <%}else{ for(var i=reqData.length-1; i>=0; i--){%>
  <div class="weui_media_box weui_media_text">
      <p class="weui_media_desc"><%=reqData[i].msg%></p>
      <ul class="weui_media_info">
          <li class="weui_media_info_meta"><%=reqData[i].time%></li>
      </ul>
  </div>
  <%}}%>
</div>
<%include footer.ejs%>
